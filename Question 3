	// 1'den 20'ye kadar olan sayıların tümüne bölünebilen en küçük pozitif tam sayıyı bulun 
	int i=1;int k=2;int t=0;
	while(!(i==t)) {i++;
	k=1;
		while(i%k==0&&k<21) {
			k++;
